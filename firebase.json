{
  "hosting": {
    "public": ".",
    "ignore": [
      ".git/**",
      ".gitignore",
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      // {
      //   "source": "/update_iata_info",
      //   "function": "update_iata_info"
      // },
      // {
      //   "source": "/od",
      //   "function": "get_from_to_page"
      // },
      // {
      //   "source": "/od.html",
      //   "function": "get_from_to_page"
      // }
      {
        // If it makes it here, it didn't match any previous routing
        // Match all routes that get to this point and send them to the home page.
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}